node {
        
        stage('Checkout'){
            git 'https://github.com/mounikamc/TestDeployment.git'
        }
        
       
        stage ('Build'){
           nodejs('nodejs'){
           bat  'npm run build'
           echo 'Build completed'
        
           }
        }
        
        
         stage ('test'){
            nodejs('nodejs'){
            bat 'npm run start'
            }
         }

         stage ('Copy'){
            
            bat 'cp -a /var/lib/jenkins/workspace/AngularPipeline/dist/SampleAngularProject/. /var/www/SampleAngularProject/html/'
            
         }
    
}

